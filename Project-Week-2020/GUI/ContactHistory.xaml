<Window x:Class="GUI.ContactHistory"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GUI"
        mc:Ignorable="d"
        Title="ContactHistory" Height="450" Width="800">
    <StackPanel Background="#171717">
        <Grid>
            <Button x:Name="tlacitko" Content="Show Contact History" HorizontalAlignment="Center" Height="38" Margin="0 259 0 0" VerticalAlignment="Top" Width="300" Click="tlacitko_Click" Background="#F25050" BorderBrush="Black"/>
          <TextBlock x:Name="ViewConnection" HorizontalAlignment="Center" Height="188" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="442" Foreground="#F25050" TextAlignment="Center" Margin="0 20 0 0"/>
        </Grid>
    </StackPanel>
</Window>
